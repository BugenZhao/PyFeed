{
  "version": "v0.3",
  "time_interval": 900,
  "box_path": "./box_bjwb.p",
  "rss_path": "/root/homepage/public/rss_t.xml",
  "url": "http://bjwb.seiee.sjtu.edu.cn",
  "base_url": "http://bjwb.seiee.sjtu.edu.cn",
  "title": "交大电院本科生教务办 RSS by Bugen",
  "description": "github.com/BugenZhao/PyFeed",
  "auto_datetime": false,
  "datetime": {
    "xpath": "//*[@id=\"layout11\"]/div/div[1]/div[2]/text()",
    "index": 0,
    "re": "\\d{4}-\\d{2}-\\d{2}",
    "fmt": "%Y-%m-%d"
  },
  "max_count": 0,
  "content": true,
  "xpath": [
    {
      "a": "//*[@id=\"layout231\"]/div/div[2]/div[2]/h4/a",
      "title": ".//text()",
      "title_index": 0,
      "href": "@href",
      "href_index": 0
    },
    {
      "a": "//*[@id=\"layout231\"]/div/div[2]/ul//li/a",
      "title": ".//text()",
      "title_index": 1,
      "href": "@href",
      "href_index": 0
    }
  ]
}